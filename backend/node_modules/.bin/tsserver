#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/bin/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules/typescript/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/typescript@5.4.5/node_modules:/mnt/g/Program Files (x86)/Matias/UTN/Desarrollo de Software/Repositorios/TP-3k02-Berli-Gilardoni-Godoy-Jaca-Marquez-App/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
